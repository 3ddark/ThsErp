--
-- Name: stk_stok_hareketi; Type: TABLE; Schema: public; Owner: ths_admin
--

CREATE TABLE public.stk_stok_hareketi (
    id bigint NOT NULL,
    stok_kodu character varying(32) NOT NULL,
    miktar numeric(18,6) NOT NULL,
    tutar numeric(18,6) NOT NULL,
    tutar_doviz numeric(18,6) NOT NULL,
    para_birimi character varying(3),
    is_giris boolean DEFAULT true NOT NULL,
    tarih timestamp without time zone NOT NULL,
    ambar_id integer NOT NULL,
    is_donem_basi boolean DEFAULT false NOT NULL
);


